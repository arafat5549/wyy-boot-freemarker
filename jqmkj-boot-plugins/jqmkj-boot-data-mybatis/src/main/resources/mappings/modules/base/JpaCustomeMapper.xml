<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jqmkj.java.common.persistence.repository.JpaCustomeRepository">


    <select id="_findByCombo" resultType="com.jqmkj.java.util.domain.ComboData">
        SELECT ${columns}
        FROM
        ${tableName}
        <where>
            ${condition}
        </where>
        <if test="sort!=null and sort!=''">
        ORDER BY ${sort}
        </if>
    </select>

</mapper>
